<?xml version="1.0" encoding="UTF-8"?>
<flow name="all" version="1.0.0" comment="全量语法示例">
    <chain name="mainChain" retryCount="5">
        THEN(IF("#{param!=null}", $func.event('a'), $end));
    </chain>
    <chain name="a" version="1.0.0">
        WHEN(b, c, d);
    </chain>
    <chain name="b" version="1.0.0">
        THEN($end);
    </chain>
    <chain name="c" version="1.0.0">
        THEN($end);
    </chain>
    <chain name="d" version="1.0.0">
        THEN(node("$end"));
    </chain>
</flow>
